{
  "name": "pick-easy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "run-p dev:*",
    "dev:client": "ng serve --proxy-config server/server.conf.json",
    "dev:server": "nodemon server/server.js",
    "debug": "run-p dev:client debug:server",
    "debug:server": "nodemon --inspect-brk server/server.js",
    "start": "node server/server.js",
    "start-prod": "run-s start:*",
    "start-prod:build": "ng build --prod",
    "start-prod:server": "cross-env NODE_ENV=production node server/server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "heroku-postbuild": "ng build --prod"
  },
  "private": true,
